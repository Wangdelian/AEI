<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8">
    <title>C1000健康表</title>
    <link rel="stylesheet" href="${siteurl}/templates/skin2/template/plugins/layui/css/layui.css" media="all">
    <script src="${siteurl}/public/js/utils.js"></script>
    <style>
        .layui-table tbody tr:hover {
            background-color: #87CEEB
        }

        .layui-table-click {
            background-color: #87CEEB;
        }

        .layui-table-cell {
            padding: 0;
            height: 18px;
            line-height: 18px;
            text-align: center;
        }

        .layui-table-view .layui-table th, .layui-table-view .layui-table td {
            padding: 5px 0;
            font-size: 12px;
            -webkit-transform-origin-x: 0;
            -webkit-transform: scale(0.90);
        }

    </style>

</head>
<body>
<br>
<div  class="layui-form layui-form-pane" id="main">
    <div class="layui-row" style="padding-left: 40px">
        <div class="layui-col-xs12">
            <div class="layui-form-item">
                <div class="layui-inline">
                    <span class="layui-form-label">TOPN</span>
                    <div class="layui-input-inline">
                        <select id="topn" name="topn" lay-search="" lay-filter="topn">
                            <option value="1">上月TOP1</option>
                            <option value="3">上月TOP3</option>
                            <option value="5">上月TOP5</option>
                        </select>
                    </div>
                </div>
                <div class="layui-inline">
                    <span class="layui-form-label">ICC</span>
                    <div class="layui-input-inline">
                        <input type="text" id="icc" name="icc" class="layui-input">
                    </div>
                </div>
            </div>
            <div class="layui-form-item">
                <!--结束日期-->
                <div class="layui-inline">
                    <label class="layui-form-label">结束日期</label>
                    <div class="layui-input-inline">
                        <input type="text" class="layui-input" id="endTime" name="endTime"  placeholder="请输入结束日期">
                    </div>
                </div>
                <%
                if(has(token_88)){
                println('<button class="layui-btn"  data-type="SearchList"  lay-submit lay-filter="search">查询</button>');
                }
                %>
            </div>
        </div>
    </div>
    <div class="layui-row" style="margin-top: 20px">
        <div class="layui-col-xs12">
            <div class="layui-inline ">
                <div id="main_echart" style="width: 1400px;height:400px;"></div>
            </div>
        </div>
    </div>
</div>

<script src="${siteurl}/templates/default/media/js/jquery1.9.1.js"></script>
<script src="${siteurl}/templates/skin2/template/plugins/layui/layui.js"></script>
<script src="${siteurl}/public/plugins/echarts/echarts.min.js"></script>
<!--业务逻辑JS-->
<script src="${siteurl}/templates/skin2/media/js/biz/MqsReport/c1000icctendency.js"></script>
</body>
<script type="text/javascript">
    var siteurl = '${siteurl}';
</script>
</html>
