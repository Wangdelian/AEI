		and productiondeptname IN
		<foreach item="item" collection="productiondeptname" open="(" separator="," close=")">
			#{item}
		</foreach>
		<if test="startTime!=null and startTime.trim()!='' ">
			and dateoccur >= to_date( #{startTime},'yyyy-MM-dd')
		</if>
		<if test="endTime!=null and endTime.trim()!='' ">
			and dateoccur &lt;= to_date(#{endTime},'yyyy-MM-dd')
		</if>
		<if test="issueseverity!=null and issueseverity.trim()!='' ">
			and issueseverity = #{issueseverity}
		</if>
		<if test="productionlinename != null and productionlinename.trim() !='' ">
			and productionlinename = #{productionlinename}
		</if>
		<if test="enginemodel != null and enginemodel.trim() !='' ">
			and enginemodel = #{enginemodel}
		</if>
		group by icc order by count desc)
		where rownum &lt; 6

	</select>

